@model IEnumerable<Contact>

<h1>Index</h1>
<a href="/contact/create">create page</a>

<br />
<br />
<br />

<table class="table">
    <thead>
        <tr>
            <th>Contact ID</th>
            <th>Contact Name</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.contactId</td>
                <td>@item.ContactName</td>
                <td>@item.Address</td>
            </tr>
        }
    </tbody>
</table>
